{
	"name": "warmindo/lapkeu-app",
	"description": "Web-Based Financial Statement System (LAPKEU) for PT. Warmindo - CodeIgniter 4",
	"type": "project",
	"homepage": "https://github.com/sfebriheri/aplikasi-lapkeu-warmindo",
	"license": "MIT",
	"require": {
		"php": "^7.4 || ^8.0",
		"codeigniter4/framework": "^4.5",
		"dompdf/dompdf": "^2.0"
	},
	"require-dev": {
		"fakerphp/faker": "^1.9",
		"phpunit/phpunit": "^9.5",
		"mikey179/vfsstream": "^1.6"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Config\\": "app/Config/"
		},
		"exclude-from-classmap": [
			"**/Database/Migrations/**"
		]
	},
	"scripts": {
		"test": "phpunit",
		"post-update-cmd": [
			"@composer dump-autoload"
		]
	},
	"support": {
		"issues": "https://github.com/sfebriheri/aplikasi-lapkeu-warmindo/issues",
		"source": "https://github.com/sfebriheri/aplikasi-lapkeu-warmindo"
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
